<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ZOA.Android.ShelfManagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity  android:name=".ActivityReader"></activity>
        <activity  android:name=".ActivityNotStandard"></activity>
        <activity  android:name=".ActivityWait"></activity>
        <activity  android:name=".ActivityOnStock"></activity>
        <activity  android:name=".ActivityStandard"></activity>
        <activity  android:name=".ActivityShelfList"></activity>
        <activity  android:name=".ActivityNextOperation"></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest>